            </div>\n                        </div>\n                        <div class=\"widget\">\n                            <h4 class=\"widget-title\">Product Brand</h4>\n                            <div class=\"sidebar-brand-list\">\n                                <ul>\n                                    <li v-for=\"item in brands\" :key=\"item.id\"><a href=\"#\">{{ item.text }} <i\n                                                class=\"fas fa-angle-double-right\"></i></a></li>\n\n                                </ul>\n                            </div>\n                        </div>\n                        <div class=\"widget has-border\">\n                            <div class=\"sidebar-product-size mb-30\">\n                                <h4 class=\"widget-title\">Product Size</h4>\n                                <div class=\"shop-size-list\">\n                                    <ul>\n                                        <li v-for=\"item in sizes\" :key=\"item.id\"><a href=\"#\">{{ item.text }}</a></li>\n\n                                    </ul>\n                                </div>\n                            </div>\n                            <div class=\"sidebar-product-color\">\n                                <h4 class=\"widget-title\">Color</h4>\n                                <div class=\"shop-color-list\">\n                                    <ul>\n                                        <li></li>\n                                        <li></li>\n                                        <li></li>\n                                        <li></li>\n                                    </ul>\n                                </div>\n                            </div>\n                        </div>\n                        <div class=\"widget\">\n                            <h4 class=\"widget-title\">Top Items</h4>\n                            <div class=\"sidebar-product-list\">\n                                <ul>\n                                    <li>\n                                        <div class=\"sidebar-product-thumb\">\n                                            <a href=\"#\"><img src=\"/front_assets/img/product/sidebar_product01.jpg\"\n                                                    alt=\"\"></a>\n                                        </div>\n                                        <div class=\"sidebar-product-content\">\n                                            <div class=\"rating\">\n                                                <i class=\"fas fa-star\"></i>\n                                                <i class=\"fas fa-star\"></i>\n                                                <i class=\"fas fa-star\"></i>\n                                                <i class=\"fas fa-star\"></i>\n                                                <i class=\"fas fa-star\"></i>\n                                            </div>\n                                            <h5><a href=\"#\">Woman T-shirt</a></h5>\n                                            <span>$ 39.00</span>\n                                        </div>\n                                    </li>\n                                    <li>\n                                        <div class=\"sidebar-product-thumb\">\n                                            <a href=\"#\"><img src=\"/front_assets/img/product/sidebar_product02.jpg\"\n                                                    alt=\"\"></a>\n                                        </div>\n                                        <div class=\"sidebar-product-content\">\n                                            <div class=\"rating\">\n                                                <i class=\"fas fa-star\"></i>\n                                                <i class=\"fas fa-star\"></i>\n                                                <i class=\"fas fa-star\"></i>\n                                                <i class=\"fas fa-star\"></i>\n                                                <i class=\"fas fa-star\"></i>\n                                            </div>\n                                            <h5><a href=\"#\">Slim Fit Cotton</a></h5>\n                                            <span>$ 39.00</span>\n                                        </div>\n                                    </li>\n                                    <li>\n                                        <div class=\"sidebar-product-thumb\">\n                                            <a href=\"#\"><img src=\"/front_assets/img/product/sidebar_product03.jpg\"\n                                                    alt=\"\"></a>\n                                        </div>\n                                        <div class=\"sidebar-product-content\">\n                                            <div class=\"rating\">\n                                                <i class=\"fas fa-star\"></i>\n                                                <i class=\"fas fa-star\"></i>\n                                                <i class=\"fas fa-star\"></i>\n                                                <i class=\"fas fa-star\"></i>\n                                                <i class=\"fas fa-star\"></i>\n                                            </div>\n                                            <h5><a href=\"#\">Fashion T-shirt</a></h5>\n                                            <span>$ 39.00</span>\n                                        </div>\n                                    </li>\n                                </ul>\n                            </div>\n                        </div>\n                    </aside>\n                </div>\n            </div>\n        </div>\n    </section>\n    <!-- shop-area-end -->\n\n\n\n</template>\n<script>\n\nimport axios from 'axios';\nimport getUrlList from '../provider';\nimport Product from '../components/Product.vue';\n\nexport default {\n    name: 'Category',\n    components: {\n\n        Product,\n    },\n    created() {\n        this.getData();\n    },\n    data() {\n        return {\n            categoryProducts: [],\n            subcategories: [],\n            brands: [],\n            sizes: [],\n            colors: [],\n        }\n    },\n    methods: {\n        async getData() {\n            const urls = getUrlList();\n            const response = await axios.get(urls.getCategoryData + '/' + this.$route.params.slug);\n            const data = response.data.data;\n            this.categoryProducts = data.categoryProducts;\n            this.subcategories = data.subcategories;\n            this.brands = data.brands;\n            this.sizes = data.sizes;\n            this.colors = data.colors;\n        }\n    },\n    watch: {\n        '$route.params.slug'(newSlug, oldSlug) {\n            // console.log('Route changed:', newSlug);\n            this.getData(newSlug); // call method to fetch data\n        }\n    },\n}\n</script>\n"
        }
    ]
