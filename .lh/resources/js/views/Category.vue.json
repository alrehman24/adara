            "content": "<template>\n\n\n\n    <!-- breadcrumb-area -->\n    <section class=\"breadcrumb-area breadcrumb-bg\" data-background=\"/front_assets/img/bg/breadcrumb_bg01.jpg\">\n        <div class=\"container\">\n            <div class=\"row\">\n                <div class=\"col-12\">\n                    <div class=\"breadcrumb-content\">\n                        <h2>Shop Sidebar</h2>\n                        <nav aria-label=\"breadcrumb\">\n                            <ol class=\"breadcrumb\">\n                                <li class=\"breadcrumb-item\"><a href=\"index.html\">Home</a></li>\n                                <li class=\"breadcrumb-item active\" aria-current=\"page\">Shop</li>\n                            </ol>\n                        </nav>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </section>\n    <!-- breadcrumb-area-end -->\n\n    <!-- shop-area -->\n    <section class=\"shop-area pt-100 pb-100\">\n        <div class=\"container\">\n            <div class=\"row\">\n                <div class=\"col-xl-9 col-lg-8\">\n\n                    <div class=\"shop-top-meta mb-35\">\n                        <div class=\"row\">\n                            <div class=\"col-md-6\">\n                                <div class=\"shop-top-left\">\n                                    <ul>\n                                        <li><a href=\"#\"><i class=\"flaticon-menu\"></i> FILTER</a></li>\n                                        <li>Showing 1â€“9 of 80 results</li>\n                                    </ul>\n                                </div>\n                            </div>\n                            <div class=\"col-md-6\">\n                                <div class=\"shop-top-right\">\n                                    <form action=\"#\">\n                                        <select name=\"select\">\n                                            <option value=\"\">Sort by newness</option>\n                                            <option>Free Shipping</option>\n                                            <option>Best Match</option>\n                                            <option>Newest Item</option>\n                                            <option>Size A - Z</option>\n                                        </select>\n                                    </form>\n                                </div>\n                            </div>\n                        </div>\n                    </div>\n                    <div class=\"row\">\n                        <div v-for=\"item in categoryProducts\" :key=\"item.id\" class=\"col-xl-4 col-sm-6\">\n                            <div class=\"new-arrival-item text-center mb-50\">\n                                <div class=\"thumb mb-25\">\n                                    <a href=\"shop-details.html\"><img :src=\"item.image\" alt=\"\"></a>\n                                    <div class=\"product-overlay-action\">\n                                        <ul>\n                                            <li><a href=\"cart.html\"><i class=\"far fa-heart\"></i></a></li>\n                                            <li><a href=\"shop-details.html\"><i class=\"far fa-eye\"></i></a></li>\n                                        </ul>\n                                    </div>\n                                </div>\n                                <div class=\"content\">\n                                    <h5><a href=\"shop-details.html\">{{ item.name }}</a></h5>\n                                    <span class=\"price\">{{ item.min_price }}</span>\n                                </div>\n                            </div>\n                        </div>\n                    </div>\n                    <div class=\"pagination-wrap\">\n                        <ul>\n                            <li class=\"prev\"><a href=\"#\">Prev</a></li>\n                            <li><a href=\"#\">1<